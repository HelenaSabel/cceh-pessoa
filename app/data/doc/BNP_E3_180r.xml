                        </rs> (<rs type="name" key="AdC" role="author">
                            <choice>
                                <abbr>Alv<am>.</am>
                                </abbr>
                                <expan>Alv<ex>aro</ex>
                                </expan>
                            </choice> de Campos</rs>) <note target="range(I1,I3)"
                            place="margin left" type="addition">
                            <label>1 vol. (I-III)</label>
                            <metamark rend="curly-bracket" function="grouping"/>
                        </note>
                        <note target="range(I2,I3)" place="margin right" type="addition">
                            <metamark rend="curly-bracket" function="grouping"/>
                            <label>Vol. II</label>
                        </note>
                    </item>
