                            <supplied resp="UH" reason="omitted-in-original">
                                <label>1 vol. (IV-VI)</label>
                            </supplied>
                            <metamark rend="curly-bracket" function="grouping"/>
                        </note>
                    </item>
                    <item xml:id="I6">
                        <label>6.</label>
                        <seg>
                            <rs type="work" key="O4" style="d"><rs type="title">Trez Odes</rs></rs>
