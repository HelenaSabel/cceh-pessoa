                            <choice>
                                <abbr>pref<am>.</am>
                                </abbr>
                                <expan>pref<ex>acio</ex>
                                </expan>
                            </choice> de <rs type="name" key="RR" role="author">
                                <choice>
                                    <abbr>R</abbr>
                                    <expan>R<ex>icardo</ex>
                                    </expan>
                                </choice> Reis</rs>
                        </rs>) <note target="I1" place="margin right" type="addition">
